import type { App } from 'vue';
<%-components.map(s=>`import ${s.name} from '${s.path}';`).join('\n')%>

export {
<%-components.map(s=>`${s.name},`).join('\n')%>
};

const install = (app: App) => {
  <%-components.map(s=>`app.component(${s.name}.name, ${s.name});`).join('\n')%>
}

/**
* 组件插件
*/
export const ComponentPlugin = {
  install,
};
